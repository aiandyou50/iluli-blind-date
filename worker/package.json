{
  "name": "iluli-worker",
  "version": "1.0.0",
  "description": "Iluli Dating Service - Cloudflare Workers Backend",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:migrate:local": "wrangler d1 execute iluli-db --local --file=./schema.sql",
    "db:migrate:prod": "wrangler d1 execute iluli-db --remote --file=./schema.sql",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.6.3",
    "ngeohash": "^0.6.3",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^22.9.3",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6",
    "wrangler": "^3.86.0"
  }
}
